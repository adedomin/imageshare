{
  "name": "image-share",
  "version": "2021.02.13",
  "description": "upload images and text files",
  "main": "index.js",
  "bin": {
    "imgshare": "./bin/imgshare.js"
  },
  "scripts": {
    "update-bulma": "cp node_modules/bulma/css/bulma.css public/css",
    "test": "echo \"Error: no test specified\" && exit 1",
