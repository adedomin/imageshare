    "busboy": "0.3.1",
    "express": "4.17.3",
    "express-slow-down": "^1.4.0",
    "file-type": "16.5.4",
    "hashids": "2.2.8",
    "mime-types": "^2.1.14"
  },
  "devDependencies": {
    "bulma": "^0.4.3",
    "synp": "1.9.10"
  }
}
